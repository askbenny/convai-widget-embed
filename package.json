    "@typescript-eslint/parser": "^8.45.0",
    "eslint": "^9.8.0",
    "husky": "9.1.7",
    "microbundle": "^0.15.1",
    "prettier": "3.6.2",
    "typescript": "^5.5.4",
    "vite": "^7.1.8"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/askbenny/convai-widget-embed.git",
    "directory": "packages/convai-widget-embed"
