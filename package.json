{
  "name": "@askbenny/convai-widget-embed",
  "version": "1.0.21",
  "description": "The Conversational AI Widget bundled with all dependencies for easy embedding.",
  "main": "./dist/index.js",
  "unpkg": "./dist/index.js",
  "source": "src/index.ts",
